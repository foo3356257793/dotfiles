use std::env;

fn main() {

    let project_dir = env::var("CARGO_MANIFEST_DIR").unwrap();

    println!("cargo:rustc-link-search={}", project_dir); // the "-L" flag
    println!("cargo:rustc-link-search=/home/dad/Documents/euler/prob_3/my_factor"); // the "-L" flag
    println!("cargo:rustc-link-lib=myfactor"); // the "-l" flag
    //println!("cargo:rustc-link-lib=flint"); // the "-l" flag
    //println!("cargo:rustc-link-lib=gmp"); // the "-l" flag

}

