#!/usr/bin/sage

if __name__ == "__main__":

    coeffs = [2,1,2]

    k = 2
    while len(coeffs) < 100:

        k += 2

        coeffs.append(1)
        coeffs.append(1)
        coeffs.append(k)

    n = 100

    i = n-1
    val = coeffs[i]

    while i > 0:

        i -= 1

        val = 1/val + coeffs[i]

    num = numerator(val)

    digits = str(num)

    answer = sum([int(c) for c in digits])
