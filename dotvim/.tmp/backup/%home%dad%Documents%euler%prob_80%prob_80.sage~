#!/usr/bin/sage

prec_needed = ceil( lb( (10**102) ) )
prec_needed += 0
RF = RealField(prec_needed)

if __name__ == "__main__":

    # get it to work for sqrt(2)
    a = RF(sqrt(2))

    a_str = str(a)

    print(a_str[2:102])

    # verify that it did this correctly

    dec_digits = [int(c) for c in a_str if c != '.']
    dec_digits = dec_digits[:100]

    print('2:', sum(dec_digits))

    bd = 100

    S = 0
    for A in range(2,bd+1):

        if is_square(A):
            continue

        a = RF(sqrt(A))
        a_str = str(a)
        dec_digits = [int(c) for c in a_str if c != '.']
        dec_digits = dec_digits[:100]

        S += sum(dec_digits)

    print(S)

