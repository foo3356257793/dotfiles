#!/usr/bin/sage

if __name__ == "__main__":

    n = 10 # number of digits

    num_perm = factorial(n)

    target_ranking = 10**6

    # first digit

    target_index = target_ranking-1

    digit_pos = []

    for i in range(n-1):

        digit = target_index // factorial(n-1-i)

        digit_pos.append(digit)

        target_index -= digit * factorial(n-1-i)

    perm = []

    remaining_digits = [i for i in range(n)]

    for d in digit_pos:

        actual_digit = remaining_digits[d]

        perm.append(actual_digit)

        for j in range(d, n-1):

            remaining_digits[j] = remaining_digits[j+1]

    perm.append(remaining_digits[0])

    perm_str = ''
    for d in perm:
        perm_str += '%d' % d

    print(perm_str)
