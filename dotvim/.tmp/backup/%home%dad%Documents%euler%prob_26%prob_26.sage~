#!/usr/bin/sage

def find_recurring_cycle(n):

    remainders = [1]

    r = 1
    while True:

        r = (10 * r) % n

        if r in remainders:

            index = remainders.index(r)

            len_cycle = len(remainders) - index

            return len_cycle

        remainders.append(r)

if __name__ == "__main__":

    d = 999
    arg_max_d = d

    max_cycle = find_recurring_cycle(d)

    while d > max_cycle:

        d -= 1
        cycle = find_recurring_cycle(d)

        if cycle > max_cycle:

            arg_max_d = d
            max_cycle = cycle

    print("d=%d" % arg_max_d)

