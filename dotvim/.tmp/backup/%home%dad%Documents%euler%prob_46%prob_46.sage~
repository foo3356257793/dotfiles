#!/usr/bin/sage

if __name__ == "__main__":

    a = 9

    while True:

        while is_prime(a):
            a += 2

        sq_bd = floor(sqrt(a-3/2))

        hit = True

        for b in range(1,sq_bd+1):

            p = a - 2*b^2

            if is_prime(p):
                hit = False
                break

        if hit:
            print(a)
            break

        a += 2

