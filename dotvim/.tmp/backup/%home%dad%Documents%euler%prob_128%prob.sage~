#!/usr/bin/sage

if __name__ == "__main__":

    ret = [1,2]

    for n in range(2,100000):

        start_diff = [6*n-1,6*n+1,12*n+5]
        end_diff = [6*n-1,6*n+5,12*n-7]

        if not is_prime(6*n-1):
            continue

        if is_prime(6*n+1) and is_prime(12*n+5):

            ret.append(2+3*n*(n-1))

        if is_prime(6*n+5) and is_prime(12*n-7):

            ret.append(1+3*n*(n+1))

    print(len(ret), ret)
