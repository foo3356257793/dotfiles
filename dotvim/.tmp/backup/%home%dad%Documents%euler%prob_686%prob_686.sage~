#!/usr/bin/sage

if __name__ == "__main__":

    # find lots of powers of 2 with leading digits 123

    # brute force?

    ct_123 = 0

    pow_2 = RDF(1)
    n = 0

    #bd = 45
    bd = 678910

    ov_10 = 1/RDF(10)

    while ct_123 < bd:

        pow_2 *= 2
        pow_str = str(pow_2)

        if pow_2 > 10**32:
            pow_2 *= ov_10

        if pow_str[0:4] == '1.23':

            ct_123 += 1

            if ct_123 % 100 == 0:
                print(ct_123, n)

        n += 1
        if n > 2**16:
            break


    print(ct_123, n)

