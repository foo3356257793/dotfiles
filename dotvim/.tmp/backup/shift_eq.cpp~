_TYPE_OUT &_TYPE_OUT::operator<<=(const _TYPE_IN in){
  if(in < 0){
    return (*this) >>= ((uint64_t) (-in));
  }
  _fun_paster(_ftype,mul_2exp)(data, data, ((uint64_t) in));
  return *this;
}

_TYPE_OUT &_TYPE_OUT::operator>>=(const _TYPE_IN in){
  if(in < 0){
    return (*this) <<= ((uint64_t) (-in));
  }
  _fun_paster(_ftype,tdiv_q_2exp)(data, data, ((uint64_t) in));
  return *this;
}
