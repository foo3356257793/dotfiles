#!/usr/bin/sage

RF = RealField(3000)

def F_a(a,n):

    val = ceil(sqrt(a)) + RF(sqrt(a))
    val = val**(n)
    val = floor(val)

    return val

def f_a(a):

    return F_a(a,a*a) % p

if __name__ == "__main__":

    """
    a = 26

    ans = F_a(a,a*a)

    d_mi = ceil(sqrt(a)) - sqrt(a)
    d_pl = 3*ceil(sqrt(a)) + sqrt(a)
    """

    a = 100

    C = 2*ceil(sqrt(a))
    lb_C = lb(C)
    lb_term = lb_C * (a^2)
    n = a*a

    print(lb_term)

    for i in range(100):

        lb_term -= lb_C
        lb_term -= 0.5
        lb_term += lb(n-i)
        lb_term -= lb(i+1)

        print(i, lb_term)
