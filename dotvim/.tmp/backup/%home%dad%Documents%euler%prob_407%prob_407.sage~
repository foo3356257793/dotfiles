#!/usr/bin/sage

import time

def find_all_possible(n):

    d_n = divisors(n)

    max_poss = 1
    for a in d_n:

        if a*a >= n:
            break

        b = n//a

        if gcd(a,b) > 1:
            continue

        ab = crt(0,1,a,b)

        if ab > max_poss:
            max_poss = ab

        ba = crt(1,0,a,b)

        if ba > max_poss:
            max_poss = ba

    return max_poss

if __name__ == "__main__":

    bd = 10**2

    start_tm = time.perf_counter()

    S = 0

    for n in range(1,bd+1):
        S += find_all_possible(n)

    end_tm = time.perf_counter()
    tot_tm = end_tm-start_tm
    print("TIME = %.3lf" % tot_tm)

