#!/usr/bin/sage

if __name__ == "__main__":

    bd = 10**6

    scores = [0.0] * (bd+1)

    primes = prime_range(bd)

    for p in primes:

        p_score = lb(p) - lb(p-1)

        for m in range(p,bd+1,p):

            scores[m] += p_score

    arg_min = scores.index(min(scores))

    print(arg_min)

