\title{Tree}
\author{}
\date{\today}

\begin{document}

\maketitle

\begin{abstract}
	I am going to make a tree.
\end{abstract}

\section{With qtree}

\Tree [.start [.$0$ [.$0$ ] [.$1$ ] [.$2$ ] ].$0$
						  [.$1$ [.$0$ ] [.$1$ ] [.$2$ ] ] ].start

\

\Tree [.A [.B [.C one ] [.D two ] ].B [.E three ] ].A

\

\Tree[.start [.$0$ [.$0$ ] [.$1$ ]] [.$1$ ] [.$2$ ]]

\section{With tikz}

\begin{tikzpicture}[level distance=10mm,sibling distance=10mm,every node/.style={fill=blue!30,circle,inner sep=5pt}
]
\node {start}
child[grow=right] {
child {node{0}
	child[grow=right]{
		child {node{0}}
		child {node{1}}
}} 
child {node{1}
	child[grow=right]{
		child {node{0}}
		child {node{1}}
}}
child {node{2}
	child[grow=right]{
		child {node{0}}
		child {node{1}}
}}
};
\end{tikzpicture}

\section{With tikz qtree}

\begin{tikzpicture}[level distance=0.50in,sibling distance=.125in,scale=.75]
\tikzset{edge from parent/.style= 
            {thick, draw,
                edge from parent fork right},every tree
							node/.style={minimum width=0.5in,text width=0.5in, align=center},grow'=right}
\Tree 
    [. A 
			[.B 
				[.C 
					[.$\ldots$ ]
				]
				[.D 
					[.$\ldots$ ]
				]
			]
			[.E ]
			[.F 
				[.G  $\ldots$
				]
				[.H 
					[.$\ldots$ ]
				]
			]
    ]

\end{tikzpicture}

