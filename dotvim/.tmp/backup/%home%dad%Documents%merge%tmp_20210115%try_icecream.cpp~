#include <dad.hpp>

#include <icecream.hpp>

int foo(int a,int b) {
  return a*b + 1;
}


int main(int argc, char *argv[]){

  print("Hello, world!\n");

  int a = 4;
  int b = 5;

  IC(a,b);

  DEBUG(43,a);
  DEBUG(44,b);
  DEBUG(45,a+b);

  IC(foo(a,b));
}

