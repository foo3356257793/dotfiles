#include <gmp.h>
#include <gmpxx.h>

#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>

#include <iostream>

using namespace std;

/*
 * 40 choose 20
 */

const int grid_size = 20;

int main()
{

  mpz_class answer = 1;

  for(int i=0; i<grid_size; i++)
  {
    answer *= (2*grid_size-i);
    answer /= (i+1);
  }

  cout << answer << "\n";

}

