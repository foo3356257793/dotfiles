#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>

#include <vector>

#include <iostream>

using namespace std;

const int bound = 10000;

void prob_21()
{

  int sum_divisors[bound+1] = {};

  //vector<int> sum_divisors;
  //sum_divisors.resize(bound+1, 0);

  for(int divisor=1; divisor<bound+1; divisor++)
  {
    int multiple = 2*divisor;

    while(multiple <= bound)
    {
      sum_divisors[multiple] += divisor;
      multiple += divisor;
    }
  }

  int answer = 0;

  for(int divisor=1; divisor<bound+1; divisor++)
  {
    int pair = sum_divisors[divisor];

    if(pair < divisor)
    {
      int copair = sum_divisors[pair];

      if (divisor == copair)
      {
        answer += divisor + pair;
      }
    }
  }

  cout << answer << endl;
}

int main(int argc, char *argv[])
{

  prob_21();

}

