#!/usr/bin/sage

if __name__ == "__main__":

    fp = open('matrix.txt', 'r')

    lines = fp.readlines()

    fp.close()

    mat = matrix(ZZ, 80, 80)

    for i in range(80):

        arr = lines[i].split(',')
        for j in range(80):

            mat[i,j] = int(arr[j])

    min_path_mat = copy(mat)

    # minimal paths on top row
    for j in range(1,80):

        min_path_mat[0,j] += min_path_mat[0,j-1]

    for i in range(1,80):

        # left edge only accessible from top
        min_path_mat[i,0] += min_path_mat[i-1,0]

        for j in range(1,80):

            min_to_here = min(min_path_mat[i-1,j], min_path_mat[i,j-1])

            min_path_mat[i,j] += min_to_here

