// NOTE: _setfun needs to correctly handle
//       the case where data, in.data are
//       the same pointer.  The Flint "set"
//       functions do this.

#ifndef _isptr
_TYPE::_TYPE(const _TYPE_IN &in) : _TYPE()
{
  _setfun(data,in.data);
}
#endif

_TYPE &_TYPE::operator=(const _TYPE_IN &in)
{
  _setfun(data,in.data);
  return *this;
}
