#!/usr/bin/sage

def resilience(d):

    return euler_phi(d) / (d-1)

if __name__ == "__main__":


    bd = 15499/94744

    # find a number below this resilience

    a = 2
    p = 2

    while resilience(a) >= bd:

        p = next_prime(p)

        # try smaller multiples
        abort = False
        for b in range(2,p):

            if resilience(b*a) < bd:
                a = b*a
                abort = True
                break

        if abort:
            break

        a *= p

    print(a)
