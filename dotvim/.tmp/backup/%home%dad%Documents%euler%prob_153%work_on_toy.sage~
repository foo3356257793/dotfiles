#!/usr/bin/sage

if __name__ == "__main__":

    ans_10_5 = 17924657155
    # ans_5 = 35

    # look through 'a' values and examine 'b' values

    max_n = 10^5
    max_a = floor(sqrt(max_n))

    S_n = 0

    for a in range(1,max_a+1):

        b_min = 1
        b_max = min(a, floor(sqrt(max_n - a^2)))

        # first pass: loop over b values

        for b in range(b_min, b_max+1):

            norm_val = a^2 + b^2

            num_multiples = floor(max_n / norm_val)

            S_n += num_multiples * (2*a + 2*b)

    for a in range(max_n+1):

        num_multiples = floor(max_n / a)

        S_n += num_multiples * a
