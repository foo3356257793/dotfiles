#include <algorithm>
#include <iostream>
#include <memory>
#include <numeric>
#include <tuple>
#include <vector>

using namespace std;

#include <fmt/core.h>
#include <fmt/ostream.h>

using fmt::print;
using fmt::format;

#include <flint/flint.h>
#include <flint/fmpzxx.h>

using namespace flint;


int main(int argc, char *argv[]){

  fmpzxx x,y;

  x = 1;
  y = 2;

  print("Hello, world!\n");

  print("{}\n", x+y);

  auto table = unordered_map<fmpzxx,int64_t>();

  for(fmpzxx index=0; index<16; index++){
    table[index] = 1;
  }

}

