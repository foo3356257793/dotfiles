#!/usr/bin/sage

if __name__ == "__main__":

    #bd = 1500000
    bd = 48

    m_bd = floor((-1+sqrt(1+2*bd))/2)

    num_triples = [0] * (bd+1)

    triples = []

    triples_by_M = [[] for _ in range(bd+1)]

    for m in range(1,m_bd+1):

        for n in range((m&1)+1,m,2):

            if gcd(m,n) > 1:
                continue

            a = m**2 - n**2
            b = 2*m*n
            c = m**2 + n**2

            L = a+b+c
            M = L

            while M <= bd:

                num_triples[M] += 1
                M += L

    ct = 0

    for L in range(bd+1):

        if num_triples[L] == 1:
            ct += 1

    print(ct)



