#!/usr/bin/sage


if __name__ == "__main__":

    bd = 10**6

    #cube_bd = floor( (2*bd)^(1/3) )
    cube_bd = 10**3

    num_cubes = len(cubes)

    good_primes = []

    p = 2^3 - 1^3
    a = 2

    while p < bd:

        if is_prime(p):
            good_primes.append(p)

        a += 1
        p = a^3 - (a-1)^3
