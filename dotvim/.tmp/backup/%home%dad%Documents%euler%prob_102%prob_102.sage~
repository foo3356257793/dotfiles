#!/usr/bin/sage

def test_triangle(t):

    mat = matrix(3,2,t)

    nv = mat.left_kernel().basis_matrix()[0]

    if nv[0] < 0:
        nv *= -1
    elif nv[0] == 0:
        if nv[1] < 0:
            nv *= -1
        elif nv[1] == 0:
            return False # not on the interior

    return nv[1] >= 0 and nv[2] >= 0


if __name__ == "__main__":

    fp = open('triangles.txt', 'r')

    lines = fp.readlines()

    fp.close()

    triangles = []

    for line in lines:

        arr = line.strip()
        arr = arr.split(',')
        arr = [int(a) for a in arr]

        triangles.append([vector(arr[2*i:2*(i+1)]) for i in range(3)])

    for i in range(2):
        print(test_triangle(triangles[i]))
