#!/usr/bin/sage

if __name__ == "__main__":

    max_val = 10**6

    grph = [[] for _ in range(max_val+1)]

    for y in range(1,max_val):

        div_y = divisors(2*y)

        head = 0
        tail = len(div_y)-1

        while head <= tail:

            f = div_y[head]
            g = div_y[tail]

            head += 1
            tail -= 1

            a = (f-g)

            if a & 1 == 1:
                continue

            a >>= 1

            b = (f+g)>>1

            x = (a^2 + b^2)/2

            if x <= max_val and x != y:
                grph[x].append(y)
                grph[y].append(x)

    hits = []
    for y in range(1,max_val):

        if len(grph[y]) >= 2:

            for x in grph[y]:

                intersection = [z for z in grph[y] if z in grph[x]]

                if len(intersection) > 0:

                    for z in intersection:

                        print(x,y,z)
                        hits.append(x+y+z)
