#!/usr/bin/sage

D = 4

dbl_pandigital_digits = []
for i in range(D):
    dbl_pandigital_digits += [i,i]

def is_dbl_pandigital(a):

    a_str = str(a)
    a_arr = [int(c) for c in a_str]
    a_arr.sort()

    return dbl_pandigital_digits == a_arr


def digit_sum(a):

    return sum([int(c) for c in a_str])

