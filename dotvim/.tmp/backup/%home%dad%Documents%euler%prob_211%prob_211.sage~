#!/usr/bin/sage

import time

if __name__ == "__main__":

    #bd = 2**22
    bd = 64000000

    start_tm = time.perf_counter()

    sig_2 = [1] * (bd+1)

    ret = [1]

    for a in range(2,bd+1):

        if a % 1000 == 0:
            print("    %d" % a)

        a_sq = a*a

        sig_2[a] += a_sq

        if is_square(sig_2[a]):
            ret.append(a)

        for m in range(2*a,bd+1,a):

            sig_2[m] += a_sq

    end_tm = time.perf_counter()

    print("time = %lf" % (end_tm-start_tm))

    print(sum(ret))
