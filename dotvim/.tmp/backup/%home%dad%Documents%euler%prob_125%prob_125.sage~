#!/usr/bin/sage

def is_palindrome(a):

    a_str = str(a)
    len_a = len(a_str)

    for i in range(len_a//2):

        if a_str[i] != a_str[len_a-1-i]:
            return False

    return True

if __name__ == "__main__":

    #bd = 10**3
    bd = 10**8

    # n^2 + (n+1)^2 <= (bd-1)
    max_start = floor(sqrt(bd))

    vals = []

    for start in range(1,max_start+1):

        next_base = start+1
        val = start*start + next_base*next_base

        while val < bd:

            if is_palindrome(val):
                vals.append(val)

            next_base += 1
            val += next_base*next_base

    print(sum(vals))
