_TYPE &_TYPE::_ADDMUL(const _TYPE_IN_NEW &in_1, const _TYPE_IN_SI in_2) {
  if(in_2 < 0){
    _native_opmul_ui(data, in_1.data, (uint64_t) (-in_2));
  }
  _native_addmul_ui(data, in_1.data, (uint64_t) (in_2));
  return *this;
}

_TYPE &_TYPE::_ADDMUL(const _TYPE_IN_UI in_2, const _TYPE_IN_NEW &in_1) {
  return _ADDMUL(in_1,in_2);
}
