#define _def
#include "def_base_funs.hpp"

  #ifdef _isptr
  _rawptr data;

  _TYPE() = delete;
  _TYPE(_raw_t other) { data = other; }

  ~_ZZ(){}
  #else
  _raw_t data;

  _TYPE(){ _initfun(data); }
  _TYPE(const _raw_t other) : _TYPE() { _setfun(data,other);}

  ~_TYPE(){ _clearfun(data); }
  #endif

#undef _def
#include "def_base_funs.hpp"

  #define _TYPE_IN _TYPE_NEW
  #include "copys.hpp"
  #include "swaps.hpp"
  #undef _TYPE_IN
  #define _TYPE_IN _TYPE_PTR
  #include "copys.hpp"
  #include "swaps.hpp"
  #undef _TYPE_IN

