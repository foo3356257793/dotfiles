fn num_poss(coins: [i32; 8], index: usize, target: i32) -> i32 {

    if index == coins.len()-1 {
        return 1;
    }

    let mut ret = 0;

    let mut t = target;
    let c = coins[index];

    while t >= 0 {
        ret += num_poss(coins, index+1, t);
        t -= c;
    }

    return ret
}

fn prob_31() -> i32
{

    let coins:[i32; 8] = [200,100,50,20,10,5,2,1];

    return num_poss(coins, 0, 200);
}


fn main() {
    let answer = prob_31();

    println!("{}", answer);
}
