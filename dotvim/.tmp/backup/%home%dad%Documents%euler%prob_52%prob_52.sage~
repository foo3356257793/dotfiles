#!/usr/bin/sage

def are_perm(a,b):

    aa = [c for c in str(a)]
    aa.sort()

    bb = [c for c in str(b)]
    bb.sort()

    return aa==bb

if __name__ == "__main__":


    a = 1

    works = False

    while not works:

        works = True

        for k in range(2,7):
            if not are_perm(a,k*a):
                works = False
                break

        a += 1

    print(a)
