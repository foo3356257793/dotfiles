#!/usr/bin/sage

def check_exponent(d, n):

    if n % 3 != 0:
        return power_mod(10,d,n)==1

    val = power_mod(10,d,9*n)
    val = (val-1)//9

    return val == 0

if __name__ == "__main__":

    target = 10**6 + 1
    #target = 10+1

    n = target

    while True:

        n += 1

        while gcd(n,10) > 1:
            n += 1

        print("...", n)

        A_n = min([d for d in divisors(euler_phi(n)) if check_exponent(d,n)])

        if A_n > target:
            print(n)
            break

