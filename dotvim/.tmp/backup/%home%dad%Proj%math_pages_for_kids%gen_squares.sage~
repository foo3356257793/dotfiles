#!/usr/bin/sage

if __name__ == "__main__":

    # for i in range(21,40+1):
    #     print("\\item $%d \\times %d = \\blank$" % (i,i))

    for i in range(1,21):

        line = '1'
        for j in range(1,i):
            line += ' + %d' % (j+1)

        line += ' = \\blank'

        print(line)




