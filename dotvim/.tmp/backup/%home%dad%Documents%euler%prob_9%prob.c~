#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>

#define SUM (1000)

int main(int argc, char *argv[])
{

    // a<=b<=c
    // given a

    #pragma omp parallel for
    for(int a=1; a<=(SUM/3); a++)
    {
        for(int b=a; b<=(SUM-a)/2; b++)
        {
            int c=SUM-a-b;
            if(a*a+b*b==c*c)
            {
                printf("%d\n", a*b*c);
            }
        }
    }

}

