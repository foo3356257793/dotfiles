#!/usr/bin/sage

"""
Project Euler Problem 20 solution.
"""

# need this to compile
import sage.functions.other.Function_factorial
#from sage.functions.other import factorial

def prob_20():

    val = factorial(100)

    str_val = "%d" % val

    sum_digits = 0

    for c in str_val:

        sum_digits += int(c)

    print("%d" % sum_digits)

if __name__ == "__main__":

    prob_20()
