  ]
  \tikzstyle{ann} = [draw=none,fill=none,right]
  {
    \node[circle,draw,minimum size=1.25cm] (v02) {$8$};
    \node[rectangle,draw,minimum size=1.25cm] (v11) [below left of=v02]
    {$12$};
    \node[rectangle,draw,minimum size=1.25cm] (v13) [below right
    of=v02] {$15$};
    \node[circle,draw,minimum size=1.25cm] (v20) [below left of=v11]
    {};
    \node[rectangle,draw,minimum size=1.25cm] (v22) [below right of=v11] {};
    \node[circle,draw,minimum size=1.25cm] (v24) [below right of=v13]
    {};

    \path[-] (v02) edge node {} (v11);
    \path[-] (v02) edge node {} (v13);
    \path[-] (v11) edge node {} (v20);
    \path[-] (v13) edge node {} (v24);
    \path[-] (v20) edge node {} (v22);
    \path[-] (v22) edge node {} (v24);

    \\
  };
\end{tikzpicture}

