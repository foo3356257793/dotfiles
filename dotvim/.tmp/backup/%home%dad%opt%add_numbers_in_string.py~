#!/usr/bin/python3

import sys
import re

if __name__ == "__main__":

    stuff = sys.stdin.readlines()

    S = 0

    for line in stuff:

        print(line, end='')

        line_list = line.split()

        for i in range(len(line_list)-1,-1,-1):

            try:
                val = float(line_list[i])
                S += val
                break
            except:
                continue

    if abs(S - round(S)) < 1e-4:
        S = int(round(S))

    max_len_line = max([len(l) for l in stuff])

    print(f'{S:>{max_len_line-1}}')


