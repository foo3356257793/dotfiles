template <typename T>
ZZ_mat(const std::vector<std::vector<T>> &in) : ZZ_mat() {
  size_t rows = in.size();
  size_t cols;
  if (rows == 0) {
    cols = 0;
  } else {
    cols = in[0].size();
  }
  fmpz_mat_init(data, rows, cols);
  for (size_t row = 0; row < rows; row++) {
    if (in[row].size() != cols) {
      throw std::invalid_argument("list of vectors not a matrix!");
    }
    for (size_t col = 0; col < cols; col++) {
      (*this)[row][col] = in[row][col];
    }
  }
}

#include "ZZ_mat_only_ZZ_mat.hpp"

