#ifndef _isptr
_TYPE::_TYPE(const _SI_TYPE in_1, const _UI_TYPE in_2) : _TYPE() {
  fmpq_set_si(data, in_1, in_2);
}

_TYPE::_TYPE(const _UI_TYPE in_1, const _SI_TYPE in_2) : _TYPE() {
  if(in_2 < 0){
    fmpq_set_ui(data,in_1,(ulong) (-in_2));
    fmpq_neg(data,data);
  }
  else{
    fmpq_set_ui(data, in_1, (ulong) (in_2));
  }
}
#endif

_TYPE &_TYPE::operator=(const std::tuple<_SI_TYPE,_UI_TYPE> &in) {
  fmpq_set_si(data, get<0>(in), get<1>(in));
  return *this;
}

_TYPE &_TYPE::operator=(const std::tuple<_UI_TYPE,_SI_TYPE> &in) {
  if(get<1>(in) < 0){
    fmpq_set_ui(data,std::get<0>(in),(ulong) (-std::get<1>(in)));
    fmpq_neg(data,data);
  }
  else{
    fmpq_set_ui(data, std::get<0>(in), (ulong) (std::get<1>(in)));
  }
}
