#!/usr/bin/sage

def reverse_int(a):

    r = 0

    while a > 0:

        s = a % 10

        r = 10 * r + s

        a //= 10

    return r

def is_odd_digits(a):

    while a > 0:

        if a & 1 == 0:
            return False

        a //= 10

    return True


if __name__ == "__main__":

    print(1234, reverse_int(1234))

    a = 36

    print(is_odd_digits(a+reverse_int(a)))
