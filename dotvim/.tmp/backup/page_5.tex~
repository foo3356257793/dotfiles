    \hline
    6 & 6 & 12 & 18 & 24 & 30 & 36 & 42 & 48 & 54\\
    \hline
    7 & 7 & 14 & 21 & 28 & 35 & 42 & 49 & 56 & 63\\
    \hline
    8 & 8 & 16 & 24 & 32 & 40 & 48 & 56 & 64 & 72\\
    \hline
    9 & 9 & 18 & 27 & 36 & 45 & 54 & 63 & 72 & 81\\
    \hline
\end{tabular}
\end{centering}
\end{table}

