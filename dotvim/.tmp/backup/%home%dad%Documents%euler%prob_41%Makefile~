CC = gcc

all: prob.o

INCLUDE = -I/usr/include/flint

prob.o: prob.cpp
	g++ -std=c++20 -g $(INCLUDE) $+ -o $@ -O3 -fopenmp -lflint -ftree-vectorizer-verbose=5
	#g++ -std=c++20 -g $(INCLUDE) $+ -o $@ -O3 -fopenmp -lflint -fdump-ipa-all

cf: prob.cpp
	clang-format -i --style=Google $+

clean:
	rm prob.o

.PHONY: clean

