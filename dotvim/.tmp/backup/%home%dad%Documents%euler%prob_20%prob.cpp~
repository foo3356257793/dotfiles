#include <gmp.h>
#include <gmpxx.h>
#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>

#include <iostream>

using namespace std;

mpz_class factorial(int n)
{
  mpz_class answer = 1;
  for(int i=1; i<=n; i++)
  {
    answer *= i;
  }

  return answer;
}

int sum_digits(mpz_class number)
{
  mpz_class answer = 0;
  while(number > 0)
  {
    answer += number % 10;
    number /= 10;
  }

  return answer.get_si();
}

void prob_20()
{
  mpz_class big_number = factorial(100);

  int answer = sum_digits(big_number);

  cout << answer << endl;

}

int main(int argc, char *argv[])
{

  prob_20();

}

