#!/usr/bin/sage

if __name__ == "__main__":

    primes = prime_range(10**6)

    bd = 10**6

    start = 0
    consec_sum = primes[start]

    end = 1

    while consec_sum < bd:

        consec_sum += primes[end]
        end += 1

    end -= 1
    consec_sum -= primes[end]

    while not is_prime(consec_sum):

        consec_sum -= primes[start]
        start += 1

        while consec_sum < bd:

            consec_sum += primes[end]
            end += 1

        end -= 1
        consec_sum -= primes[end]

    print(consec_sum)
