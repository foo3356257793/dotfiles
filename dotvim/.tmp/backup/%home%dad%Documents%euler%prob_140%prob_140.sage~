#!/usr/bin/sage

if __name__ == "__main__":

    sol = []

    num_pts = 30

    poly_ZZ.<x,y> = ZZ[]

    starting_points = [[2,-7],[0,-1],[0,1],[-4,5],[-3,2],[-3,-2]]

    recursion = [ [-9*x-4*y-14,-20*x-9*y-28], [-9*x+4*y-14,20*x-9*y+28] ]

    sol = copy(starting_points)

    for tup in starting_points:

        a = tup[0]
        b = tup[1]

        for poly_tup in recursion:

            f = poly_tup[0]
            g = poly_tup[1]

            for i in range(num_pts):

                t = f(x=a,y=b)
                b = g(x=a,y=b)
                a = t

                if not [a,b] in sol:
                    sol.append([a,b])

    n_sol = []
    for tup in sol:

        n = tup[0]
        if n > 0:
            n_sol.append(n)

    n_sol.sort()
