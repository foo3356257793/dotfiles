#include <iostream>
using std::cout;
using std::endl;

#include <math.h>

#include <vector>
using std::vector;

#include <iterator>
using std::next;

int find_num_possible(vector<int>::iterator coins, vector<int>::iterator end, int target)
{

  if(coins == end)
  {
    return 1;
  }

  int answer = 0;

  int t = target;
  int c = (*coins);

  while(t >= 0)
  {
    answer += find_num_possible(next(coins), end, t);
    t -= c;
  }

  return answer;
}

void prob_31() {

  vector<int> coins = {200,100,50,20,10,5,2};

  int answer = find_num_possible(coins.begin(),coins.end(),200);

  cout << answer << endl;
}

int main(int argc, char *argv[]) { prob_31(); }
