CC = gcc

CXX = g++ -std=c++20 -O3 -g
CFLAGS =
INCLUDE = -I/usr/include/flint  -I/home/dad/.local/include
LIBS = -lm -lflint -lgmp -lgtest -lpthread

all: prob_10 test_prob_10

prob_10: prob.cpp
	$(CXX) $(CFLAGS) $(INCLUDE) $+ -o $@ $(LIBS)

test_prob_10: test_prob.cpp
	$(CXX) $(CFLAGS) $(INCLUDE) $+ -o $@ $(LIBS)

clean:
	rm prob_10 test_prob_10

.PHONY: clean

