#!/usr/bin/sage

if __name__ == "__main__":

    a = ZZ(19)

    p = a.popcount()

    E_win = [1,3,4,7,15,16,31]

    for i in range(1,a):

        pop_i = ZZ(i).popcount()

        j = a - i

        pop_j = ZZ(j).popcount()

        if (p + pop_i + pop_j) % 2 == 0:
            print('%d -> even' % i)
            continue

        elif j in E_win:
            print(i,'mapsto',j)

