#!/usr/bin/sage

def frac_below_val(target, d):

    #
    # want n/d < target
    # as close as possible

    # would have n = d*target at equality
    # n = floor(d*target) does it

    n = floor(d * target)
    if n/d == target:

        n-=1

    return n/d

if __name__ == "__main__":

    closest = 2/5

    for d in range(8,1000000+1):

        f = frac_below_val(3/7, d)

        if f > closest:
            closest = f



    print(numerator(closest))
