UPPER_BD = 1000 - 1

max_mul_3 = UPPER_BD ÷ 3

sum_3 = 3 * ((max_mul_3 * (1 + max_mul_3)) ÷ 2);

max_mul_5 = UPPER_BD ÷ 5;
sum_5 = 5 * ((max_mul_5 * (1 + max_mul_5)) ÷ 2);

max_mul_15 = UPPER_BD ÷ 15;
sum_15 = 15 * ((max_mul_15 * (1 + max_mul_15)) ÷ 2);

answer = sum_3 + sum_5 - sum_15;

println(answer)
