#!/usr/bin/sage

if __name__ == "__main__":

    poly_ZZ.<x> = ZZ[]

    f = 3*x^2 - 2x - 1

    for p in prime_range(3,100):

        good = []
        for r in range(p):

            ff = f(x=r) % p
            if ff==0 or legendre_symbol(ff,p) == 1:
                good.append(r)

        print(p, len(good))
