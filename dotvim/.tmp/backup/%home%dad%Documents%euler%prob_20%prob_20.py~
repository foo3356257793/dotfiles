#!/usr/bin/sage -python

"""
Project Euler Problem 20 solution.
"""

import sage.all
from sage.functions.other import factorial

if __name__ == "__main__":

    val = factorial(100)

    str_val = "%d" % val

    SUM_DIGITS = 0

    for c in str_val:

        SUM_DIGITS += int(c)

    print("answer is %d" % SUM_DIGITS)
