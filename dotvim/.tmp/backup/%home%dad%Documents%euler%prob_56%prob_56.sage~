#!/usr/bin/sage

if __name__ == "__main__":

    max_sum = 0
    arg_max = [0,0]

    for a in range(1,100):

        for b in range(1,100):

            c = a ** b

            c_digits = str(c)

            digit_sum = sum([int(d) for d in c_digits])

            if digit_sum > max_sum:
                max_sum = digit_sum
                arg_max = [a,b]
