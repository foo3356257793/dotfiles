#![allow(non_upper_case_globals)]
#![allow(non_camel_case_types)]
#![allow(non_snake_case)]
#![allow(improper_ctypes)]

include!("bindings.rs");

fn main() {
    println!("Hello, world!");

    let mut p_contents:fmpz = 0;
    let p_ptr = &mut p_contents as *mut fmpz;

    let mut exp_contents:mp_limb_t = 0;
    let exp_ptr = &mut exp_contents as &mut mp_limb_t;

    let mut factor = fmpz_factor_struct { sign : 0, p : p_ptr, exp : exp_ptr, alloc : 0, num : 0} ;

    unsafe {
        let factor_ptr = &mut factor as *mut fmpz_factor_struct;
        fmpz_factor_init(factor_ptr); }


    let x = 600851475143i64;
    let X:*mut fmpz;

    unsafe {
        let factor_ptr = &mut factor as *mut fmpz_factor_struct;
        fmpz_factor_clear(factor_ptr); }
}
