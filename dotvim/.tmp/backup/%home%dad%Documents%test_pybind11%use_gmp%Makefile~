all: test_gmp.cpython-39-x86_64-linux-gnu.so

CXX = g++ -std=c++20 -O3 -Wall

CFLAGS =
INCLUDE = -I/usr/include/python3.9 -I/home/dad/.local/lib/python3.9/site-packages/pybind11/include
LIBS = -lgmp

test_gmp.cpython-39-x86_64-linux-gnu.so: test.cpp
	$(CXX) -shared -fPIC $(INCLUDE) $+ -o $@ -fopenmp $(LIBS)

clean:
	rm test_gmp.cpython-39-x86_64-linux-gnu.so

.PHONY: clean

