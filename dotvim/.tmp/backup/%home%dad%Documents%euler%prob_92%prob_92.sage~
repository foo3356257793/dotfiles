#!/usr/bin/sage

def chain_end(a):

    if a == 1 or a == 89:

        return a

    a_str = str(a)

    next_val = sum([int(c)^2 for c in a_str])

    return chain_end(next_val)

if __name__ == "__main__":


    num_89 = 0

    for a in range(1,10**7):

        v = chain_end(a)

        if v == 89:

            num_89 += 1

