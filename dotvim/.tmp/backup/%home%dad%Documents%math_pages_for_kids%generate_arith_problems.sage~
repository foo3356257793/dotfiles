#!/usr/bin/sage

if __name__ == "__main__":

    num_prob = 20

    probs = []

    for _ in range(num_prob):

        a = randrange(10)
        b = randrange(10)

        tup = [a,b]
        tup.sort()

        while tup in probs:

            a = randrange(10)
            b = randrange(10)

            tup = [a,b]
            tup.sort()

        probs.append(tup)

    for tup in probs:

        flip = randrange(2) 

        print("\\item %d + %d = \\blank" % (tup[flip], tup[(flip+1)&1]))
