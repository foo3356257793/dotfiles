using AbstractAlgebra
using Nemo

# extend the factor function to Int64s
import AbstractAlgebra: factor

function factor(target::Int64)
    return factor(ZZ(target))
end

function prob_3(target)

    target = ZZ(target)

    fac = factor(target)

    answer = 1
    for (p, mul) in fac
        if p > answer
            answer = p
        end
    end

    println(answer)

end

target = 600851475143

prob_3(target)
