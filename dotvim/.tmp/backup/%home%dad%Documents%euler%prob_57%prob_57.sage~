#!/usr/bin/sage

if __name__ == "__main__":

    a = 1

    num_hit = 0

    d = 2

    for i in range(10):

        val = 1 + 1/d

        print(val)

        num = numerator(val)
        den = denominator(val)

        num_digits = ceil(log(num,10))
        den_digits = ceil(log(den,10))

        if num_digits > den_digits:
            num_hit += 1

        d = 2 + 1/d


    print(num_hit)
