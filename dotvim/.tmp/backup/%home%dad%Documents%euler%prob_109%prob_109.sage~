#!/usr/bin/sage

import itertools

if __name__ == "__main__":

    dbl_values = [2*a for a in range(1,21)] + [50]

    non_dbl_values = list(range(1,21))
    non_dbl_values += [3*a for a in range(1,21)]
    non_dbl_values += [25]

    all_values = dbl_values + non_dbl_values

    init_turn_values = [0] + all_values

    # two of the same
    init_turn_values += [2*a for a in all_values]

    # two different
    for tup in itertools.combinations(all_values, 2):

        init_turn_values.append(sum(tup))


    init_turn_values.sort()
    dbl_values.sort()

    num_checkout = 0

    for a in init_turn_values:
        if a >= 100:
            break
        for b in dbl_values:

            if a+b >= 100:
                break
            num_checkout += 1

