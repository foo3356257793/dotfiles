#include <iostream>
using std::cout;
using std::endl;

#include <math.h>

double A = 0.5*(1+sqrt(5));
const int target_log = 1000-1;

// exec prob_25
void prob_25()
{

  // A^n/sqrt(5) >= 10^target_log
  // n*log10(A) - 0.5*log10(5) = target_log
  // n = (target_log + log10(5))/log10(A)
  int answer = ceil((target_log+0.5*log10(5)) / log10(A));

  cout << answer << endl;

}

int main(int argc, char *argv[]) {

  prob_25();

}
