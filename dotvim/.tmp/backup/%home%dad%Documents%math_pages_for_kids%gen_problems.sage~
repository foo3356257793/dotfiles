#!/usr/bin/sage

if __name__ == "__main__":

    num_probs = 10

    p = 4

    vals = []

    for i in range(num_probs):

        works = randrange(2)

        a = randrange(100//p)
        a *= p
        if works:

            while a in vals:
                a += p

            vals.append(a)

        else:

            a += randrange(1,p)

            while a in vals:
                a += p

            vals.append(a)


    vals.sort()

    for a in vals:
        print("\\item Is $%d$ in the %d club? {\\bf Yes} or {\\bf No}." % (a,p))
