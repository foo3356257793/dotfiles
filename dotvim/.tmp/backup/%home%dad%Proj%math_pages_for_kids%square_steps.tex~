\begin{multicols}{2}

  \begin{itemize}
    \item $0 \times 0 = \blank$
    \item $1 \times 1 = \blank$
    \item $1 + \cdots + 8 = 6 \times 6$\\
      \\
      $6 \times 6 = \blank$
    \item $1 + \cdots + 49 = 35 \times 35$\\
      \\
      $35 \times 35 = \blank$
    \item $1 + \cdots + 288 = 204 \times 204$\\
      \\
      $204 \times 204 = \blank$
    \item $1 + \cdots + 1681 = 1189 \times 1189$\\
      \\
      $1189 \times 1189 = \blank$
    \item $1 + \cdots + 9800 = 6930 \times 6930$\\
      \\
      $6930 \times 6930 = \blank$
    \item $1 + \cdots + 57121 = 40391 \times 40391$\\
      \\
      $40391 \times 40391 = \blank$
    \item $1 + \cdots + 332928 = 235416 \times 235416$\\
      \\
      $235416 \times 235416 = \blank$
    % \item $1 + \cdots + 1940449 = 1372105 \times 1372105$\\
    %   \\
    %   $1372105 \times 1372105 = \blank$
    % \item $1 + \cdots + 11309768 = 7997214 \times 7997214$\\
    %   \\
    %   $7997214 \times 7997214 = \blank$
\end{itemize}

\end{multicols}

