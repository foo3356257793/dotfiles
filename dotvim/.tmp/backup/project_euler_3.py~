#! /usr/bin/python

 from math import sqrt

number = 600851475143
factor = 2
maxFactor = 1
while factor <= sqrt(number):
  if number % factor == 0:
    maxFactor = factor
    number /= factor
  else:
    factor += 1
if number > maxFactor:
  maxFactor = number
else:
  print "%s not greater than %s" %(number, maxFactor)

print number

