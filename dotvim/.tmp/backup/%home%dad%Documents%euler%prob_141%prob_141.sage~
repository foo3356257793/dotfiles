#!/usr/bin/sage

def is_geom_seq(a,b,c):

    if a <= 0 or b <= 0 or c <= 0:
        return False

    a,b,c = sorted([a,b,c])

    return c == b*b/a

if __name__ == "__main__":

  # find decomposition for 10404

  a = 10404

  for q in range(1,102):

      d = a // q
      r = a - q*d

      if is_geom_seq(q,r,d):

          print(q,d,r)
