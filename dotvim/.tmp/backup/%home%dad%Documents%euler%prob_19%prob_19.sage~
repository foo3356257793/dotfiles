#!/usr/bin/sage

def prob_19():

    months = [31,28,31,30,31,30,31,31,30,31,30,31]

    months = [m % 7 for m in months]

    day = 1
    month = 0

    num_sundays = 0

    year = 1900

    while year <= 2000:

        day += months[month]

        if month == 1:
            # check for leap
            if year % 4 == 0:
                day += 1

        day %= 7

        month += 1

        if month >= 12:
            month -= 12
            year += 1

        if day == 0:
            if year > 1900:
                num_sundays += 1


    print("%d" % num_sundays)

prob_19()
