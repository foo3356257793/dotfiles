#!/usr/bin/sage

def find_max_idempotent(n):

    for a in range(n-1,0,-1):

        if (a*a) % n == a:
            return a

    print('n=%d: returning 0' % n)
    return 0

if __name__ == "__main__":

    bd = 10**2

    S = 0

    for n in range(1,bd+1):

        S += find_max_idempotent(n)

    print(S)
