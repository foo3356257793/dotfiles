#include <stdio.h>
#include <stdlib.h>
#include <omp.h>

int main(int argc, char *argv[])
{

    omp_set_num_threads(4);

    long int s = 0;

    #pragma omp parallel for
    for(int i=0; i<1000; i++)
    {
        long int f=1;
        long int g=i;
        long int h;
        for(int j=0; j<1000; j++)
        {
            h=f+g;
            f=g;
            g=h;
        }

        #pragma omp atomic
            s += g;
    }

    printf("s=%ld\n", s);

}

